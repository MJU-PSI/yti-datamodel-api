    <skos:prefLabel xml:lang="en">Construction</skos:prefLabel>
    <skos:prefLabel xml:lang="sl">Construction</skos:prefLabel>
    <skos:prefLabel xml:lang="fi">Rakentaminen</skos:prefLabel>
