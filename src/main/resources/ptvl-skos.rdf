    <skos:prefLabel xml:lang="en">Tourism in Finland</skos:prefLabel>
    <skos:prefLabel xml:lang="sl">Tourism in Finland</skos:prefLabel>
    <skos:note xml:lang="fi">Tähän palvelualaluokkaan kuuluvat kotimaan matkailun tieto-, reitti- ja majoituspalvelut.</skos:note>
    <skos:broader rdf:resource="http://urn.fi/URN:NBN:fi:au:ptvl:v1152"/>
