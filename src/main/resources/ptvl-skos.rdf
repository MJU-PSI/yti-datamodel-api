    <skos:broader rdf:resource="http://urn.fi/URN:NBN:fi:au:ptvl:v1159"/>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:notation>P19.3</skos:notation>
  </rdf:Description>
