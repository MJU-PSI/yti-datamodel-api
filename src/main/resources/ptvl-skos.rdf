    <skos:prefLabel xml:lang="sl">Authorisations to enter and documents</skos:prefLabel>
    <skos:inScheme rdf:resource="http://urn.fi/URN:NBN:fi:au:ptvl:"/>
    <skos:broader rdf:resource="http://urn.fi/URN:NBN:fi:au:ptvl:v1174"/>
    <sh:order>2201</sh:order>
    <skos:notation>P22.1</skos:notation>
    <skos:prefLabel xml:lang="fi">Maahantuloluvat ja asiakirjat</skos:prefLabel>
